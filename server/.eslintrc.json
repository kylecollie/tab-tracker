{
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "off",
    "linebreak-style": ["error", "windows"],
    "comma-dangle": "off",
    "array-bracket-newline": "error",
    "no-unused-vars": "warn"
  }
}
